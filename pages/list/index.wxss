page{
    background-color: #f2f2f2;
}
.list-box{
    text-align: center;
}
.list-box image{
    width: 96%;
    height: 240px;
    margin: 20rpx 0;
}
.list-box-title{
   font-size: 20px;
   color: #919292; 
   margin-bottom: 20rpx;
}
.list-box-content{
    display: flex;
    flex-wrap: nowrap;
    justify-content: center;
    margin-bottom: 20rpx;
}
.list-box-content view{
    font-size: 12px;
    color: #ccc;
}