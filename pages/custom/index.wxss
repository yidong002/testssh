.custom-title{
    display: flex;
    flex-wrap: nowrap;
    justify-content: space-around;
    margin-top: 20rpx;
}
.custom-title>view{
    display: flex;
    flex-direction: column;
    justify-content: center;
}
.title-id{
    width:32px;
    height: 32px;
    border: 1px solid #445356 ;
    background-color: #445356;
    color: #fff;
    border-radius: 50%;
    text-align: center;
    line-height: 32px;
}
.title-content{
    margin-top: 20rpx;
    font-size: 12px;
    text-align: center;
}